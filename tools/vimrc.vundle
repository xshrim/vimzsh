func SetComment(ext)
    let l:type = tolower(a:ext)
    if l:type ==? 'c' || l:type ==? 'h' || l:type ==? 'cpp' || l:type ==? 'hpp' || l:type ==? 'java' || l:type ==? 'php' || l:type ==? 'sql' || l:type ==? 'css' || l:type ==? 'js' || l:type ==? 'cs' || l:type ==? 'go'
        let l:head = '/*****************************************************************'
        let l:tail = '*****************************************************************/'
        let l:sym = '**'
    elseif l:type ==? 'sh' || l:type ==? 'py' || l:type ==? 'conf' || l:type ==? 'ini'
        let l:head = '##****************************************************************'
        let l:tail = '##****************************************************************'
        let l:sym = '#*'
    elseif l:type ==? 'htm' || l:type ==? 'html' || l:type ==? 'xml'
        let l:head = '<!--**************************************************************'
        let l:tail = '***************************************************************-->'
        let l:sym = '**'
    elseif l:type ==? 'jsp'
        let l:head = '<%--**************************************************************'
        let l:tail = '**************************************************************--%>'
        let l:sym = '**'
    elseif l:type ==? 'asp'
        let l:head = "'<%--**************************************************************"
        let l:tail = "'*************************************************************--%>"
        let l:sym = "'*"
    elseif l:type ==? 'rb'
        let l:head = '=begin ***********************************************************'
        let l:tail = '************************************************************* =end'
        let l:sym = '**'
    elseif l:type ==? 'pl'
        let l:head = '=pod *************************************************************'
        let l:tail = '************************************************************* =cut'
        let l:sym = '**'
    else
        let l:head = '##****************************************************************'
        let l:tail = '##****************************************************************'
        let l:sym = '##'
    endif

    call setline(1,l:head)
    call append(line('.'),   l:sym.'   Copyright (C) '.strftime('%Y').' All rights reserved.')
    call append(line('.')+1, l:sym.'   ')
    call append(line('.')+2, l:sym.'   æ–‡ä»¶åç§°ï¼š'.expand('%:t'))
    call append(line('.')+3, l:sym.'   åˆ› å»º è€…ï¼šMaple')
    call append(line('.')+4, l:sym.'   åˆ›å»ºæ—¥æœŸï¼š'.strftime('%Yå¹´%mæœˆ%dæ—¥'))
    call append(line('.')+5, l:sym.'   æ    è¿°ï¼š')
    call append(line('.')+6, l:sym.'')
    call append(line('.')+7, l:sym.'   æ›´æ–°æ—¥å¿—ï¼š')
    call append(line('.')+8, l:sym.'')
    call append(line('.')+9, l:tail)

endfunc

